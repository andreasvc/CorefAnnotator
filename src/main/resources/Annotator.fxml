<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.unistuttgart.ims.coref.annotator.Annotator">
  <children>
      <TitledPane animated="false" text="Main">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0">
               <children>
                  <FlowPane alignment="TOP_CENTER" hgap="10.0" vgap="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Button defaultButton="true" mnemonicParsing="false" onAction="#fileOpenDialog" text="%action.open" />
                        <Button mnemonicParsing="false" text="%action.quit" />
                        <Button mnemonicParsing="false" text="%action.help" />
                        <Button mnemonicParsing="false" text="%action.show.log" />
                        <Button mnemonicParsing="false" text="%action.compare" />
                        <Button mnemonicParsing="false" text="%action.merge_files" />
                     </children>
                  </FlowPane>
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
      <TitledPane animated="false" text="Import">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" />
        </content>
      </TitledPane>
  </children>
</VBox>
